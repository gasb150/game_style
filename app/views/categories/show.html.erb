
<% @articles_cat.each do |article|%>
  <ul>
    <img src="#{<%= article.image_url%>}">
    <%= article.title %>
    <%= article.text %>
    <%= article.user.name %>
    <% category = @category %>
    
    <div> <%= vote_unvote_btn(article, @category.id) %>
      <%= article.votes.count %>
      </div>
</ul>
<% end %>