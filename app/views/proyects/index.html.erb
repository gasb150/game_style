<p id="notice"><%= notice %></p>

<h1>Proyects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Framework</th>
      <th>Language</th>
      <th>Skill</th>
      <th>Other</th>
      <th>Imageurl</th>
      <th>Live url</th>
      <th>Source url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyects.each do |proyect| %>
      <tr>
        <td><%= proyect.name %></td>
        <td><%= proyect.framework_id %></td>
        <td><%= proyect.language_id %></td>
        <td><%= proyect.skill_id %></td>
        <td><%= proyect.other %></td>
        <td><%= proyect.imageurl %></td>
        <td><%= proyect.live_url %></td>
        <td><%= proyect.source_url %></td>
        <td><%= link_to 'Show', proyect %></td>
        <td><%= link_to 'Edit', edit_proyect_path(proyect) %></td>
        <td><%= link_to 'Destroy', proyect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyect', new_proyect_path %>
